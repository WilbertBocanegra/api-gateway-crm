#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/bocanegra/Development/api-gateway-express/node_modules/.pnpm/@apollo+protobufjs@1.2.2/node_modules/@apollo/protobufjs/bin/node_modules:/home/bocanegra/Development/api-gateway-express/node_modules/.pnpm/@apollo+protobufjs@1.2.2/node_modules/@apollo/protobufjs/node_modules:/home/bocanegra/Development/api-gateway-express/node_modules/.pnpm/@apollo+protobufjs@1.2.2/node_modules/@apollo/node_modules:/home/bocanegra/Development/api-gateway-express/node_modules/.pnpm/@apollo+protobufjs@1.2.2/node_modules:/home/bocanegra/Development/api-gateway-express/node_modules/.pnpm/node_modules:/home/bocanegra/Development/api-gateway-express/node_modules:/home/bocanegra/Development/node_modules:/home/bocanegra/node_modules:/home/node_modules:/node_modules:/home/bocanegra/Development/api-gateway-express/node_modules/.pnpm/node_modules/@apollo/protobufjs/bin/node_modules:/home/bocanegra/Development/api-gateway-express/node_modules/.pnpm/node_modules/@apollo/protobufjs/node_modules:/home/bocanegra/Development/api-gateway-express/node_modules/.pnpm/node_modules/@apollo/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/bocanegra/Development/api-gateway-express/node_modules/.pnpm/@apollo+protobufjs@1.2.2/node_modules/@apollo/protobufjs/bin/node_modules:/home/bocanegra/Development/api-gateway-express/node_modules/.pnpm/@apollo+protobufjs@1.2.2/node_modules/@apollo/protobufjs/node_modules:/home/bocanegra/Development/api-gateway-express/node_modules/.pnpm/@apollo+protobufjs@1.2.2/node_modules/@apollo/node_modules:/home/bocanegra/Development/api-gateway-express/node_modules/.pnpm/@apollo+protobufjs@1.2.2/node_modules:/home/bocanegra/Development/api-gateway-express/node_modules/.pnpm/node_modules:/home/bocanegra/Development/api-gateway-express/node_modules:/home/bocanegra/Development/node_modules:/home/bocanegra/node_modules:/home/node_modules:/node_modules:/home/bocanegra/Development/api-gateway-express/node_modules/.pnpm/node_modules/@apollo/protobufjs/bin/node_modules:/home/bocanegra/Development/api-gateway-express/node_modules/.pnpm/node_modules/@apollo/protobufjs/node_modules:/home/bocanegra/Development/api-gateway-express/node_modules/.pnpm/node_modules/@apollo/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@apollo/protobufjs/bin/pbts" "$@"
else
  exec node  "$basedir/../@apollo/protobufjs/bin/pbts" "$@"
fi
