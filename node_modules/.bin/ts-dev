#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/bocanegra/Development/api-gateway-express/node_modules/.pnpm/ts-dev@2.1.14/node_modules/ts-dev/node_modules:/home/bocanegra/Development/api-gateway-express/node_modules/.pnpm/ts-dev@2.1.14/node_modules:/home/bocanegra/Development/api-gateway-express/node_modules/.pnpm/node_modules:/home/bocanegra/Development/api-gateway-express/node_modules:/home/bocanegra/Development/node_modules:/home/bocanegra/node_modules:/home/node_modules:/node_modules:/home/bocanegra/Development/api-gateway-express/node_modules/ts-dev/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/bocanegra/Development/api-gateway-express/node_modules/.pnpm/ts-dev@2.1.14/node_modules/ts-dev/node_modules:/home/bocanegra/Development/api-gateway-express/node_modules/.pnpm/ts-dev@2.1.14/node_modules:/home/bocanegra/Development/api-gateway-express/node_modules/.pnpm/node_modules:/home/bocanegra/Development/api-gateway-express/node_modules:/home/bocanegra/Development/node_modules:/home/bocanegra/node_modules:/home/node_modules:/node_modules:/home/bocanegra/Development/api-gateway-express/node_modules/ts-dev/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  --unhandled-rejections=strict --no-warnings --enable-source-maps --experimental-specifier-resolution=node "$basedir/../ts-dev/tsDev.js" "$@"
else
  exec node  --unhandled-rejections=strict --no-warnings --enable-source-maps --experimental-specifier-resolution=node "$basedir/../ts-dev/tsDev.js" "$@"
fi
